{
  "expo": {
    "name": "OmNomNom",
    "slug": "OmNomNom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "DEBUG": true,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fac020bc-db25-4082-acd5-34e83f847290"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adamdelezuch89.przepisy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    }
  }
}
